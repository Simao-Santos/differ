{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        // https://github.com/expressjs/generator/issues/78#issuecomment-232890226
        "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
    }
}
